@CHARSET "UTF-8";
*{
    font-family: "Microsoft YaHei", sans-serif;
}
#page-questionBank{
    padding: 12px;
}

#page-questionBank>h4{
    padding: 8px;
    border-bottom: 1px solid gainsboro;
    margin-bottom: 20px;
}
#content-questionBank{
font-size: 14px;
    padding-left: 8px;
    padding-top: 0px;
    padding-bottom: 20px;
    border-bottom: 1px solid gainsboro;
}
#content-questionBank>select{
    width: 200px;
    height: 25px;
    margin-right: 28px;

}
#content-questionBank>button{
    background-color: #F65036;
    color: white
}
#content-questionBank>button,#contentTwo>button{
    width: 110px;
    height: 30px;
    outline: none;
    border: none;
    border-radius: 3px;
}
/*/////////////////////two////////////*/
#contentTwo{
    padding-left: 8px;
    padding-top: 20px;
    padding-bottom: 25px;
   /*  border-bottom: 1px solid gainsboro; */
}
#contentTwo>button{
    margin-right: 35px;
    background-color: white;
    border: 1px solid #E9E9EE;

}
#contentTwo>button:first-child{
    background-color: #F65036;
    color: white;
}
#contentTwo>button:nth-child(2){
    background-color: #39D1C0;
    color: white;
    padding: 0;
  font-size: 14px;
    line-height: 30px;
}
