/*modal*/
#QuesModal>p{
    width: 71%;
    text-align: right;
}

#QuesModal>p>select,#QuesModal>p>input{
    width: 245px;
    border-radius: 3px;
    border: 1px solid gainsboro;
}
#QuesModal>p>select{
    margin-right: 81px;
    height: 26px;
}
#QuesModal #knowledgepointp{
    margin-left: -70px;
}
#QuesModal p #modal-input{
    width: 130px;
    /*background-color: white;*/
    color: #F87768;
    /*border:1px solid #F87768 ;*/
    outline: none;
    height: 28px;
    resize: none;
    display: inline-block;
    float: left;
    margin: -35px 0 0 400px;
    position: relative;
    z-index: 20;
    opacity: 0;
    cursor: pointer;
}
#QuesModal #Upload-btn{
    display: inline-block;
    position: absolute;
    top: 113px;
    z-index: 10;
    width: 80px;
    border:1px solid #F87768 ;
    background-color: white;
    color: #F87768;
    cursor: pointer;
}

.Ques-txtEditor{
    width: 100%;
    margin-bottom: 20px;
}
#quesOption>button{
    width: 120px;
    height: 34px;
    border-radius: 3px;
    background-color: #39D1C0;
    color: white;
    border: none;
    margin-bottom: 15px;
}
#quesRow>div>span{
  display: inline-block;
    width: 20px;
    height: 20px;
    border-radius: 20px;
    border: 1px solid gainsboro;
    text-align: center;
    line-height: 20px;
}
.Editor-editor{
    height: 100px;
}
#txtEditor{
    width: 300px !important;
}
/*modal-footer*/
.modal-footer>button{
    width: 90px;
}
.btn-primary {
    color: black;
    background-color:white;
    border-color: #ADADAD;
}
.btn-primary:hover{
    background-color: #F65036;
    border-color: #F65036;
}
.mykpcontrol .myKpselection img{
	margin:5px 0 0 10px;
	display:inline-block;
	color:#C2C2C2;
	width:20px;
}