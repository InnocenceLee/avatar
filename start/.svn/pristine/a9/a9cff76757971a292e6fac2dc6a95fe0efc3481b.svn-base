    background: url(../images/page-prev-disabled.png);
}
.main-content .ui-pager .ui-pager-wrap .ui-page-num a.next {
    border: none;
    background: url(../images/page-next.png);
    vertical-align: 1px;
}
.main-content .ui-pager .ui-pager-wrap .ui-page-num a.next.disabled {
    background: url(../images/page-next-disabled.png);
}

.main-content .ui-pager .ui-pager-wrap .ui-page-num a {
    color: #2d94ff;
    border-color: #2d94ff;
}
.main-content .ui-pager .ui-pager-wrap .ui-page-num a:hover {
    text-decoration: none;
}

.main-content .ui-pager .ui-pager-wrap .ui-page-num a.disabled {
    background-color: #f7f6f4;
    color: #dad9d9;
}

.main-content .ui-pager .ui-pager-wrap .ui-page-num a.disabled:hover {
    background-color: #f7f6f4;
    border-color: #d6d3d3;
}

.main-content .ui-pager .ui-pager-wrap .ui-page-num input {
    padding: 0 6px;
    height: 20px;
    line-height: 20px;
    width: 28px;
    margin: 0 5px;
    text-align: center;
    font-size: 12px;
    border: 1px solid #d6d3d3;
    border-radius: 1px;
}
.main-content .target-page-btn{
    color: white !important;
    background-color:#4d90fe ;
}
.main-content .target-page-btn:hover{
    color: white !important;
}
.main-content .ui-pager .ui-pager-wrap .ui-page-num .target-page-btn {
    border-radius: 2px;
    width: 40px;
}

.main-content .ui-pager .ui-pager-wrap .ui-page-num a.current, .ui-pager .ui-pager-wrap .ui-page-num span.current {
    color: #fff;
    border-color: #4d90fe;
    background-color:#4d90fe;
}

.main-content .ui-pager .ui-pager-wrap .ui-page-num a.current:hover, .ui-pager .ui-pager-wrap .ui-page-num span.current:hover {
    color: #fff;
}

.main-content .ui-pager .ui-pager-wrap .ui-page-num .tpage-ellipsis {
    border: none;
}

.main-content .ui-pager .ui-pager-wrap .ui-page-num .prev, .ui-pager .ui-pager-wrap .ui-page-num .next {
    font-weight: bold;
}
