#contentTwo .mybtnL1{
    background-color: #37D1C1;
    border: 1px solid #D4D3D8;
    border-bottom-right-radius: 0;
    border-top-right-radius: 0;
    margin-top:-15px;
}
#contentTwo .mybtnL2{
    background-color: #FFFFFF;
    border: 1px solid #D4D3D8;
    margin-left: -5px;
    margin-bottom: 15px;
}
