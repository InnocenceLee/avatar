@charset "utf-8";

/*font*/
@font-face {
	font-family: 'ElegantIcons';
	src:url('fonts/ElegantIcons.eot');
	src:url('fonts/ElegantIcons.eot?#iefix') format('embedded-opentype'),
		url('fonts/ElegantIcons.woff') format('woff'),
		url('fonts/ElegantIcons.ttf') format('truetype'),
		url('fonts/ElegantIcons.svg#ElegantIcons') format('svg');
	font-weight: normal;
	font-style: normal;
}

/* common */
* { word-wrap: break-word; }
ul,ol,li,span,p,form,h1,h2,h3,4,h5,h6,dl,dt,dd { margin: 0; padding: 0; border: 0; z-index:inherit; }
img,a img { border:0; margin:0; padding:0; }
ul,ol,li { list-style:none; }
* { margin:0; padding:0; }
html,body { height:100%; font:12px/1.6  Microsoft YaHei, Helvetica, sans-serif; color:#333; }
input,select,textarea,button { font:14px/1.5  Microsoft YaHei, Helvetica, sans-serif; }
body, ul, ol, li, dl, dd, p, h1, h2, h3, h4, h5, h6, form, fieldset, .pr, .pc { margin: 0; padding: 0; }
table { empty-cells: show; border-collapse: collapse; }
	caption, th { text-align: left; font-weight: 400; }
ul li{ list-style: none; }
h1, h2, h3, h4, h5, h6 { font-size: 1em; }
em, cite, i { font-style: normal; }
	a img { border: none; }
label { cursor: pointer; }

.finp,.ftea,.fsel{ background:none;}


a,a:hover,a:visited  { color:#333; text-decoration:none; }

.cbtn,.cbtn a{ line-height:36px; color:#fff; background:#5ecc68; border:0; border-radius: 5px; font-size:14px; text-align:center;}

.lbtn a{ line-height:40px; background:#f1f1f1; font-size:16px;border-radius: 5px; font-size:14px;display:block; text-align:center; margin-bottom:10px;}

.btnlist{ padding:0 5px 10px 5px;}
.btnlist li{ float:left; width:50%;} 
.btnlist li a{ margin:0 5px; display:block; height:36px; line-height:36px; background:#f1f1f1; text-align:center;border-radius: 5px; font-size:14px;}

.z { float: left; } .y { float: right; }
.cl:after { content: "."; display: block; height: 0; clear: both; visibility: hidden; } .cl { zoom: 1; }
.overflow{overflow:hidden;}
.none { display:none; }
.vm { vertical-align: middle; }
	.vm * { vertical-align: middle; }
.hm { text-align: center; }

.ptb{ padding:10px 0}
.nb{ border-bottom:0px !important;}

body{ background:#f1f1f1}
.wp{ padding-bottom:46px;}
