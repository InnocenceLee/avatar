/**/
.thread_ioc{ font-size: 12px;padding:3px 5px;-moz-border-radius:8px; -webkit-border-radius:8px; border-radius:8px; color:#fff; margin-right:5px; font-weight:400}
.thread_pin{ background:#F30}
.thread_dig{ background:#09F}
.thread_special{ background:#390}


/*header*/
.header{ background:#333; color:#fff; height:50px; position:fixed; width:100%; z-index:99; overflow:hidden; left:0px; top:0px;}
.header h2{ font-size:16px; padding:0 10px; line-height:50px; font-weight:100; overflow:hidden}

/*footer*/
.footer{ width:100%; position:fixed; left:0px; bottom:0px; /*line-height:45px;*/ background:#fcfcfc; border-top:1px solid #e5e5e5; z-index:99 }
.footer em{ font-family:"ElegantIcons";}
.footer .footer_back{ width:35px; height:42px; text-align:center; border-right:1px solid #e5e5e5; font-size:32px;}
.footer .footer_nv{ width:35px; height:42px; text-align:center; border-left:1px solid #e5e5e5; font-size:32px; position:relative}

.footer ul{ position:relative;}
.footer ul li{ float:left; width:20%; text-align:center; font-size:14px;    line-height: 18px;}
.footer a{ display:block}
.footer ul li a em{font-size:20px;margin-right:2px /*font-size:18px; padding-right:6px;*/ }

.footer ul a.footer_user { position:absolute; top:-20px; z-index:100; width:42px; height:42px;-moz-border-radius:29px; -webkit-border-radius:29px; border-radius:29px;padding:8px; background:#fff;}
.footer ul a.footer_user img{ width:42px; height:42px;-moz-border-radius:21px; -webkit-border-radius:21px; border-radius:21px;}

.footer ul li.footer_forums a{ margin-right:0px;}
.footer ul li.footer_post a{ margin-left:0px;}
.footer ul span.newpm { z-index:101; position:absolute; top:-20px; background:#f00; padding:0 5px; font-size:12px; height:16px; line-height:16px; color:#fff;-moz-border-radius:8px; -webkit-border-radius:8px; border-radius:8px;}
/*pop*/
.pop_form{ background:#fff; padding:10px 15px; border-radius: 5px; min-width:240px;}
.pop_form p{ padding:5px 0;}

.pop_form p.lab label{ padding-right:5px;}
.pop_form p.lab label input{ vertical-align:middle; margin-right:5px; }
.pop_form p.lab .input{ width:30px; padding:3px 5px; border:1px solid #e5e5e5; margin-right:5px;}

.pop_form p.txt{ color:#f30; font-size:14px; line-height:20px;}
.pop_form p.inp { border:1px solid #e5e5e5; padding:3px 5px; margin:10px 0; height:26px; line-height:26px;}
.pop_form p.inp input{ width:100%; border:0px; height:20px;} 

.pop_form p.tea { border:1px solid #e5e5e5; padding:3px 5px; margin:10px 0; }
.pop_form p.tea textarea{ width:100%; border:0px; line-height:20px;} 

.pop_form p.sel{ border:1px solid #e5e5e5; padding:3px 5px;}
.pop_form p.sel select{ width:100%; border:0px; height:26px; line-height:26px; color:#999}

.pop_form p.btn{ text-align:center;}
.pop_form p.btn .cbtn{ padding:0 10px; margin-right:10px;}

/*forumlist*/
.forumlist{ margin-bottom:10px}
.forumlist .subforumshow{ height:45px; line-height:45px; background: -moz-linear-gradient( top,#fff,#f1f1f1); background: -webkit-linear-gradient( top,#fff,#f1f1f1);background: linear-gradient( top,#fff,#f1f1f1); border-bottom:1px solid #e5e5e5; }
.forumlist .subforumshow h2{ padding:0 15px; font-size:16px; font-weight:100; }
